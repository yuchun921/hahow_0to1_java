package codegym;

public class TestDemo {

	public static void main(String[] args) {
		int[] nums = {1,2,3,4,5,6};
		
		System.out.println(nums[2]);
		
		// Array is object
		// 可以指定長度
		int[] nums1 = new int[3];
		
		// 超過的話會噴exception
		nums1[0] = 2;
		nums1[1] = 4;
		nums1[2] = 6;
		
		/* 多維陣列 [行][列] */
		int[][] numnum =new int[3][2];
		numnum[0][0] = 1;
		numnum[0][1] = 2;

		numnum[1][0] = 3;
		numnum[1][1] = 4;
		
		numnum[2][0] = 5;
		numnum[2][1] = 6;
		
		// 一開始就給初始值
		int[][] numnum1 = {{1,2},{3,4,5},{6,7,8}};
		System.out.println(numnum1[2][0]);
	}

}
