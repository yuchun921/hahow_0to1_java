package codegym;

public class TestDemo {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int age_now = 28;
		int age_retire = 65;
		int year_now = 2022;

		int year_retire = year_now + (age_retire - age_now);
		System.out.println("Retire year: " + year_retire);
	}

}
