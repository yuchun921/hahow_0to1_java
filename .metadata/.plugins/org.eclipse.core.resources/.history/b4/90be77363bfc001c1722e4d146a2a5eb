package codegym;

public class hw {

	public static String gender = "";

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		String name = "Peggy";
		int age = 27;
		gender = "female";

		System.out.println("My name is " + name + ", I'm " + age + " years old, and I'm " + gender + ".");
	}

}
