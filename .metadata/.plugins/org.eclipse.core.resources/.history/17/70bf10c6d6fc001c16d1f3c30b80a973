package codegym;

public class TestDemo {

	public static void main(String[] args) {
		int[] nums = {1,2,3,4,5,6};
		
		System.out.println(nums[2]);
		
		// Array is object
		// 可以指定長度
		int[] nums1 = new int[3];
		nums1[0] = 2;
		nums1[1] = 4;
		nums1[2] = 6;
		
		System.out.println(nums1);

	}

}
